{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "share-point-emailer-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/sharePointEmailer/SharePointEmailerWebPart.js",
          "manifest": "./src/webparts/sharePointEmailer/SharePointEmailerWebPart.manifest.json"
        }
      ]
    },
    "social-buttons-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/socialButtons/SocialButtonsWebPart.js",
          "manifest": "./src/webparts/socialButtons/SocialButtonsWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "SharePointEmailerWebPartStrings": "lib/webparts/sharePointEmailer/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "SocialButtonsWebPartStrings": "lib/webparts/socialButtons/loc/{locale}.js"
  }
}
